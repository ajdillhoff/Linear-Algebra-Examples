   "source": [
    "import numpy as np\n",
    "\n",
    "X = np.random.rand(4, 3)\n",
    "D, P = np.linalg.eig(X.T @ X)\n",
    "sort_idx = D.argsort()[::-1]\n",
    "D = D[sort_idx]\n",
    "P = P[:,sort_idx]\n",
    "P = X @ P\n",
    "P = P / np.linalg.norm(P, axis=0)\n",
    "print(P)\n",
    "\n",
    "U, S, V = np.linalg.svd(X, full_matrices=False)\n",
    "print(U)"
   ]
  }
